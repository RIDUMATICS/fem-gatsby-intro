{
  "name": "frontendmasters-gatsby",
  "private": true,
  "description": "Bare-bones starter for the Gatsby Frontend Masters workshop.",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "cross-env GATSBY_GRAPHQL_IDE=playground gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "analyze": "cross-env ANALYZE_BUNDLE_SIZE=true gatsby build"
  },
  "dependencies": {
    "@emotion/core": "10.0.16",
    "@emotion/styled": "10.0.15",
    "@mdx-js/mdx": "1.3.2",
    "@mdx-js/react": "1.3.2",
    "gatsby": "^2.13.50",
    "gatsby-background-image": "0.8.9",
    "gatsby-image": "2.2.18",
    "gatsby-mdx": "1.0.0",
    "gatsby-plugin-emotion": "4.1.4",
    "gatsby-plugin-mdx": "1.0.33",
    "gatsby-plugin-react-helmet": "3.1.5",
    "gatsby-plugin-sharp": "2.2.21",
    "gatsby-plugin-webpack-bundle-analyzer": "1.0.5",
    "gatsby-remark-images": "3.1.22",
    "gatsby-source-filesystem": "2.1.18",
    "gatsby-source-instagram": "0.5.1",
    "gatsby-transformer-sharp": "2.2.14",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "5.2.1"
  },
  "devDependencies": {
    "cross-env": "5.2.1",
    "prettier": "^1.18.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jlengstorf/frontendmasters-gatsby"
  }
}
